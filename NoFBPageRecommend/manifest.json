{
    "name":"No Facebook Page Recommand",
    "version":"0.1.2",
    "manifest_version":2,

    "description":"Remove recommended page on Facebook news feed.",

    "icons":{
        "16":"nofbpage16.png",
        "48":"nofbpage48.png",
        "128":"nofbpage128"
    },

    "content_scripts":[{
        "matches":["https://*.facebook.com/*","http://*.facebook.com/*"],
        "css":["remove.css"],
        "js":["remove.js"]
    }],

    "web_accessible_resources":["kerker.js"],

    "homepage_url":"https://github.com/qcl/NoFacebookPageRecommend"
}
